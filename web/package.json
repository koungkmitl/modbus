    "lint-staged": "^8.0.4",
    "prettier": "^1.14.3",
    "serve": "^10.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,md,css}": [
